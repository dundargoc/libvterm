local vt=init()
local screen = wantscreen(vt, "") 

-- Selective erase
reset(state,screen)
push "A\x1b[1\"qB\x1b[\"qC"
  ?screen_row 0 = "ABC"
push "\x1b[G\x1b[?J"
  ?screen_row 0 = " B"

-- Non-selective erase
reset(state,screen)
push "A\x1b[1\"qB\x1b[\"qC"
  ?screen_row 0 = "ABC"
push "\x1b[G\x1b[J"
  ?screen_row 0 = ""
