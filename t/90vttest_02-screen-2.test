# TAB setting/resetting
local vt=init()
local state = wantstate(vt, "")
local screen = wantscreen(vt, "")

reset(state,screen)

push "\x1b[2J\x1b[3g"

push "\x1b[1;1H"
$REP 26: push "\x1b[3C\x1bH"

push "\x1b[1;4H"
$REP 13: push "\x1b[0g\x1b[6C"

push "\x1b[1;7H"
push "\x1b[1g\x1b[2g"

push "\x1b[1;1H"
$REP 13: push "\t*"

push "\x1b[2;2H"
$REP 13: push "     *"

-- Output
?screen_row 0 = "      *     *     *     *     *     *     *     *     *     *     *     *     *"
?screen_row 1 = "      *     *     *     *     *     *     *     *     *     *     *     *     *"

?cursor = 1,79
