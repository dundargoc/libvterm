local vt=init()
local state = wantstate(vt, "")

reset(state,screen)

-- RIS homes cursor
push "\x1b[5;5H"
  ?cursor = 4,4
local state = wantstate(vt, "") +m
push "\x1bc"
  ?cursor = 0,0
local state = wantstate(vt, "") -m

-- RIS cancels scrolling region
push "\x1b[5;10r"
local state = wantstate(vt, "") +s
push "\x1bc\x1b[25H\n"
  scrollrect 0..25,0..80 => +1,+0
local state = wantstate(vt, "") -s

-- RIS erases screen
push "ABCDE"
local state = wantstate(vt, "") +e
push "\x1bc"
  erase 0..25,0..80
local state = wantstate(vt, "") -e

-- RIS clears tabstops
push "\x1b[5G\x1bH\x1b[G\t"
  ?cursor = 0,4
push "\x1bc\t"
  ?cursor = 0,8
