local vt=init()
local screen = wantscreen(vt, "") p

reset(state,screen)
  settermprop 1 true
  settermprop 2 true
  settermprop 7 1

-- Cursor visibility
push "\x1b[?25h"
  settermprop 1 true
push "\x1b[?25l"
  settermprop 1 false

-- Title
push "\x1b]2;Here is my title\a"
  settermprop 4 ["Here is my title"]
